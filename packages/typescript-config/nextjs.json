{
	"$schema": "https://json.schemastore.org/tsconfig",
	"display": "Next.js",
	"extends": "./base.json",
	"compilerOptions": {
		"plugins": [{ "name": "next" }],
		"module": "ESNext",
		"moduleDetection": "force",
		"moduleResolution": "Bundler",
		"allowJs": true,
		"jsx": "preserve",
		"noEmit": true
	}
}
